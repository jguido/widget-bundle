<div class="row right-align" id="spinner-{{ id }}">
    <span>
        <div class="spinner">
            <div class="rect1"></div>
            <div class="rect2"></div>
            <div class="rect3"></div>
            <div class="rect4"></div>
            <div class="rect5"></div>
        </div>
    </span>
</div>
<div data-src="{{ url|raw }}" id="{{ id }}" class="widget-interface col-xs-12" data-loaded="false">

</div>
<script>
    var errorLoading = '{{ 'async_include_error'|trans({}, 'widgets') }}';
    var errorDomLoading = '{{ 'async_dom_loading_error'|trans({}, 'widgets') }}';
    jQuery(document).ready(function ($) {
        Widget.refresh("{{ id }}");
    });
</script>
